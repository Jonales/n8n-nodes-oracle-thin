{
  "name": "n8n-nodes-oracle-thin",
  "version": "1.1.0-rc.7",
  "description": "Oracle Database node com recursos avan√ßados para cargas pesadas e Oracle 19c+. Arquitetura refatorada com TypeScript moderno.",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "oracle-thin",
    "oracle-database",
    "oracle-advanced",
    "plsql",
    "bulk-operations",
    "oracle-19c",
    "oracle-23c",
    "connection-pool",
    "transactions",
    "oracle-cloud",
    "vector-search",
    "chat-memory",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "require": "./dist/lib/*.js"
    }
  },
  "files": [
    "dist",
    "lib",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run build:assets",
    "build:watch": "tsc -p tsconfig.json --watch",
    "build:assets": "gulp build:assets",
    "dev": "npm run build:watch",
    "lint": "eslint lib/**/*.ts --max-warnings 0",
    "lint:fix": "eslint lib/**/*.ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/oracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/oracle-database-advanced.node.js",
      "dist/nodes/oracle-vector-store.node.js",
      "dist/nodes/chat-memory.node.js"
    ]
  },
  "dependencies": {
    "oracledb": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^29.7.0",
    "typescript": "^5.7.2",
    "rimraf": "^6.0.1"
  }
}